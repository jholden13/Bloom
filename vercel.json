{
  "buildCommand": "pnpx convex deploy --cmd 'pnpm run build:deploy'",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/ensearch/(.*)",
      "destination": "/ensearch/index.html"
    },
    {
      "source": "/ensearch",
      "destination": "/ensearch/index.html"
    },
    {
      "source": "/trips/(.*)",
      "destination": "/trips/index.html"
    },
    {
      "source": "/trips",
      "destination": "/trips/index.html"
    }
  ]
}
